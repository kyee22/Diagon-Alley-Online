<h1>Please Login</h1>
<div style="color: red"><%= alert %></div>
<%= form_with url: do_login_users_path do %>
  <p>
    Username: <%= text_field_tag :username %>
  </p>
  <p>
    Password: <%= password_field_tag :password %>
  </p>
  <%= submit_tag 'Login' %>
  <div>
    <%= link_to "Register", new_user_path %>

  </div>

<% end %>
